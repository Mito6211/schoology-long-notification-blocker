{
    "name": "Schoology Long Notification Blocker",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://*.schoology.com/home/notifications",
                "https://*.schoology.com/home/notifications?from_popup&"
            ],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ]
}
